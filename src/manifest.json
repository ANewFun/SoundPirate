{
  "name": "声海盗",
  "version": "1.5.0",
  "description": "下载在线音乐插件，支持豆瓣，虾米，Jing.fm，SongTaste，落网，人人电台，QQ音乐，网易云音乐，新浪乐库，搜狗音乐，QQ音乐电台等",
  "manifest_version": 2,
  "permissions": ["webRequest", "activeTab","tabs", "webRequestBlocking",
                  "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["content_style.css"],
      "js": ["jquery-2.0.2.min.js", "content_script.js"]
    }
  ],
  "web_accessible_resources": [
       "images/music16.png",
       "images/music24.png",
       "images/music32.png",
       "images/music48.png",
       "images/arrowl32.png",
       "images/arrowr32.png",
       "images/m4a32.png",
       "images/aac32.png"
  ],
  "icons" : {
    "16" : "images/music16.png",
    "48" : "images/music48.png",
    "128": "images/music128.png"
  }
}
